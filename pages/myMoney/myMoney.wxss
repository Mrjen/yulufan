/* pages/myMoney/myMoney.wxss */
.container{
  width: 100%;
  min-height: 1230rpx;
  text-align: center;
  justify-content: flex-start;
  background-image: -webkit-linear-gradient(0deg, #b2fffc 0, #96cfff 100%);
  background-image: linear-gradient(90deg, #b2fffc 0, #96cfff 100%);
}

.money{
  font-size: 114rpx;
  font-weight: bold;
  color: #fff;
  position: relative;
  margin: 40rpx auto 10rpx auto;
}

.money::before{
  content: "元";
  display: block;
  font-size: 50rpx;
  font-weight: bold;
  position: absolute;
  right: -60rpx;
  bottom: 14rpx;
}

.money_tip{
  color: #fff;
  margin-bottom: 70rpx;
}

.get_money{
  display: flex;
  align-items: center;
  justify-content: flex-start;
  background: #fff;
  height: 60rpx;
  border-radius: 6rpx;
  border:1px solid #bfe4ff;
  box-sizing: border-box;
  font-size: 42rpx;
  width: 580rpx;
  padding-left: 20rpx;
  margin-bottom: 40rpx;
}

.get_money input{
  display: inline-block;
  font-size: 40rpx;
  height: 60rpx;
  width: 150rpx;
  text-align: right;
  box-sizing: border-box;
  margin-right: 20rpx;
}

.get_tip{
  font-size: 26rpx;
  text-align: left;
  margin:100rpx auto 100rpx auto;
  max-width: 500rpx;
}

.get_tip1{
  font-size: 26rpx;
  text-align: left;
  margin-top:0;
  max-width: 500rpx;
}

.get_money_btn{
    background: #07ba39;
    color: #fff;
    padding:10rpx;
    box-sizing: border-box;
    border-radius: 6rpx;
    font-size: 36rpx;
    height: 62rpx;
    margin-right: 20rpx;
}

.get_money_box{
  display: flex;
  width: 100%;
  margin-top: 40rpx;
  justify-content: center;
  align-items: center;

}

.money_help{
  width: 34rpx;
  height: 34rpx;
  display: block;
  margin-right: -54rpx;
  color: transparent;
  background-image:url('data:image/png;base64,');
  background-size: 34rpx 34rpx;
}

.input_or_output{
  text-align: center;
  font-size: 26rpx;
  color: #007ec7;
  text-decoration: underline;
  margin-top: 20rpx;
}

/*上传微信收款码*/
.upMoneyImg{
  width: 500rpx;
  height: 60rpx;
  overflow: hidden;
  background: #fff;
  border:1px solid #eee;
  color: #666;
  font-size: 26rpx;
  line-height: 60rpx;
  text-align: center;
  /* background-image:url('data:image/png;base64,');
  background-size:289rpx 60rpx;
  background-repeat: no-repeat;
  background-position:center; */
}

.up_img_box{
  display: flex;
  justify-content: center;
  align-items: center;
}

.quest{
  width:40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  margin-left: 20rpx;
  border-radius: 50%;
  color: #999;
  border:1px solid #999;
}

.ercode_box{
  width: 500rpx;
  height: 500rpx;
  margin-right: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  overflow: hidden;
}

.ercode{
  display:block;
  max-width:500rpx;
  margin:0 auto;
}

.zhanwei{
  width: 750rpx;
  height: 150rpx;
}
