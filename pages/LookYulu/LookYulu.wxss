/* pages/LookYulu/LookYulu.wxss */
.container{
  width: 100%;
  height: 100%;
  display: block;
  position: absolute;
  justify-content: flex-start;
  /*background-image: -webkit-linear-gradient(0deg, #b2fffc 0, #96cfff 100%);
  background-image: linear-gradient(90deg, #b2fffc 0, #96cfff 100%);*/
}

.yulu_item{
   margin-top: 36rpx;
   box-shadow: 0 3px 8px 0 rgba(0,0,0,0.08) ;
}

.auth {
  width:100%;
  text-align:center;
  margin-top:10rpx;
  font-size:28rpx;
  color: #5d5d5d;
  box-sizing:border-box;
  padding-right:30rpx;
}

.date_box {
  color:#444;
  text-align:left;
  font-size:30rpx;
}

.date_box image.erwei{
  width: 260rpx;
  height: 260rpx;
  display: block;
  margin:0 auto;
  margin-bottom: 20rpx;
  border-radius: 0;
}


.yulu_img{
  margin-top:60rpx;
  line-height: 50rpx;
  margin-bottom: 60rpx;
}

.image_box{
  position: relative;
}

.img_out{
  width: 642rpx;
  margin:0 auto;
}

.picture{
  width: 676rpx;
  display: block;
  margin:0 auto;
  margin-top: 16rpx;
  position: absolute;
  z-index: 1;
  left: 50%;
  transform: translate(-50%);
  border-radius: 2rpx;
}

.this_time{
   position: absolute !important;
   left: 0;
   top: 30rpx;
   transform: rotate(90deg);
   z-index: 20;
}

.label_box{
  margin-top: 100rpx;
}

.line_box{
  width: 606rpx;
  border:2px solid #333;
  position: absolute;
  z-index: 2;
  left: 50%;
  transform: translate(-50%);
  top: -10rpx;
}

.yu_text{
  font-size:32rpx;
  width: 640rpx;
  line-height: 1.5;
  left: 50%;
  transform: translate(-50%);
  position:absolute;
  z-index:7;
  background: rgba(0, 0, 0, 0.5);
  color: #fff;
  box-sizing: border-box;
  padding:20rpx 30rpx;
  text-align: justify;
  bottom: -20rpx;
}

.info_box{
  display: flex;
  justify-content: space-between;
}

.mark_box{
  font-size: 22rpx;
  color: #999;
  height: 28rpx;
  margin-top: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.mark_box::before{
  content: "";
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  margin-right: 18rpx;
  background-image:url('data:image/png;base64,');
  background-size: 30rpx 30rpx;
  background-repeat: no-repeat;
}

.radio{
  display: block;
  box-sizing:border-box;
  padding:10rpx 30rpx;
}

.download_btn {
  width:300rpx;
  height:80rpx;
  line-height:80rpx;
  text-align:center;
  font-size:26rpx;
  border-radius:6rpx;
  color:#666;
  margin:80rpx auto;
  background:#96cfff;
  color: #fff;
}


/*底部导航*/
.opation_box{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  display: flex;
  background: #9ed3ff;
  z-index: 100;
}

.downLoad{
  flex: 1;
  background: url(https://yulufan.playonwechat.com/static/icon/LookYuLu_download.png);
  background-size: 54rpx;
  background-position: center center;
  background-repeat: no-repeat;
}

.copy{
  flex: 1;
  background: url(https://yulufan.playonwechat.com/static/icon/LookYuLu_copy.png);
  background-size: 54rpx;
  background-position: center center;
  background-repeat: no-repeat;
}

.share{
  flex: 1;
  background: url(https://yulufan.playonwechat.com/static/icon/LookYuLu_share.png);
  background-size: 54rpx;
  background-position: center center;
  background-repeat: no-repeat;
}

.share_btn{
  display: block;
  width: 100%;
  height: 100rpx;
  opacity: 0;
  content: " ";
}

.zhanwei{
  width: 750rpx;
  height: 250rpx;
}

.date_box{
  position: relative;
}
.erwei_wrap{
  padding:0;
  margin: 0;
}
.date_box image.personImg{
  width: 100rpx;
  height: 100rpx;
  margin: 0;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -50rpx;
  margin-left: -50rpx;
  border-radius: 50%;
}
