/* pages/writeYulu/writeYulu.wxss */
page{
  background-color: #80baea;
}

.container{
  width: 100%;
  min-height: 1214rpx;
  justify-content: flex-start;
  background-image: -webkit-linear-gradient(0deg, #b2fffc 0, #96cfff 100%);
  background-image: linear-gradient(90deg, #b2fffc 0, #96cfff 100%);
}

.main_box{
  width: 600rpx;
  min-height: 694rpx;
  background: #fff;
  box-sizing: border-box;
  padding:20rpx;
  margin-top: 30rpx;
  border-radius: 10rpx;
}

.yu_tip{
  width:100%;
  padding:20rpx;
  box-sizing:border-box;
  font-size:30rpx;
  background: #f7f7f7;
}

.image_box{
  line-height: 100%;
  text-align: center;
  position: relative;
  justify-content: center;
  align-items: center;
  display: flex;
  border-radius: 10rpx;
}

.upload_img{
  background-image:url('data:image/png;base64,');
  background-size: 248rpx 38rpx;
  background-position: center center;
  background-repeat: no-repeat;
  border:1px solid #80baea;
  margin: 20rpx auto;
}


.image_box image{
  max-width: 100%;
}

.textText{
  display: block;
  width: 100%;
  min-height: 100rpx;
  margin-top: 20rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding:20rpx;
  font-size: 32rpx;
  border:1px solid #80baea;
}

.tag_box{
  display:flex;
  justify-content:center;
}

.label_tag{
  display:inline-block;
  font-size: 32rpx;
  width:200rpx;
  height: 60rpx;
  margin:10rpx;
  text-align:center;
  line-height:60rpx;
  border:1px solid #eee;
  border-radius:6rpx;
  box-sizing:border-box;
}

.select_tag{
  border:1px solid #80baea;
}

.upload_btn{
  width: 600rpx;
  height: 80rpx;
  text-align: center;
  line-height: 54rpx;
  background: #fff;
  font-size: 28rpx;
  background:#80baea;
  color: #fff;
  line-height:80rpx;
  text-align:center;
  border-radius:4rpx;
  margin:0 auto;
  margin-top:50rpx;
}

.upload_tip{
  font-size: 26rpx;
  margin-top: 8rpx;
  margin-bottom: 30rpx;
}
