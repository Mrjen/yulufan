/* pages/mine/mine.wxss */
.container{
  background-image:url('data:image/png;base64,');
  background-size: 100%;
  position: absolute;
  width: 100%;
  height: 100%;
  justify-content: flex-start;
}

.persion_img{
  width: 222rpx;
  height: 222rpx;
  display: block;
  border-radius: 50%;
  background-size: 222rpx 222rpx;
  background-image: url(https://yulufan.playonwechat.com/static/img/logo_150x150.png);
  margin:150rpx auto 32rpx auto;
}

.persionName{
  font-size: 30rpx;
  color: #80baea;
  margin-bottom: 130rpx;
}

.info_box{
  width: 750rpx;
  box-sizing: border-box;
  padding:0 20rpx;
  display: flex;
  margin-bottom: 200rpx;
}

.info_box navigator{
  flex:1;
  text-align: center;
  color: #80baea;
}

.info_box text{
  display: block;
  position: relative;
}
.info_item{
  position: relative;
}

.uploadWorks{
  width: 270rpx;
  height: 98rpx;
  border-radius: 8rpx;
  background-image: -webkit-linear-gradient(-90deg, #b6e1ff 0, #94ceff 100%);
  text-align: center;
  line-height: 98rpx;
  color: #fff;
}

.notice{
  width: 50rpx;
  height: 30rpx;
  background: red;
  text-align: center;
  line-height: 30rpx;
  color: #fff;
  position: absolute;
  right: 24rpx;
  font-size: 26rpx;
  border-radius: 15rpx;
}

.auth_btn{
  width: 150rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  color: #fff;
  border-radius: 2px;
  background: #80baea;
  margin-bottom:130rpx;
}
